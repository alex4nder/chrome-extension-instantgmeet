{
  "manifest_version": 3,
  "name": "Instant Google Meet",
  "version": "1.0",
  "description": "Instant Google Meet lets you create Google Meet links in one click and start meetings right from your browser.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
